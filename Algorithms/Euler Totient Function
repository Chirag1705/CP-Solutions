void computeTotient(ll n){
      ll phi[n+1];
      
      for(ll i = 0; i<=n; i++){
            phi[i] = i;
      }
      
      for(ll p = 2; p <= n; p++){
            if(phi[p] == p){
                phi[p] = p-1;
                
                for(ll i = 2*p; i <= n; i+=p){
                      phi[i] = (phi[i]/p) - (p-1);
                }
            }
      }
}
